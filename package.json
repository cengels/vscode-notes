{
    "name": "vscode-notes",
    "displayName": "VSCode Notes",
    "description": "A custom .nt extension allowing list-based note highlighting",
    "publisher": "cengels",
    "version": "1.0.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/cengels/vscode-notes.git"
    },
    "engines": {
        "vscode": "^1.41.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "nt",
            "aliases": ["Note", "nt"],
            "extensions": [".nt",".note"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "nt",
            "scopeName": "text.note",
            "path": "./syntaxes/nt.tmLanguage.json"
        }]
    }
}
