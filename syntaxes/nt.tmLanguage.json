{
    "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
    "name": "Note",
    "patterns": [
        {
            "include": "#tokens"
        },
        {
            "include": "#headings"
        },
        {
            "include": "#items"
        }
    ],
    "repository": {
        "headings": {
            "name": "entity.name.section.nt",
            "match": "^.*:$"
        },
        "items": {
            "patterns": [{
                "name": "support.type.property-name.nt",
                "match": "^[^:]+:(?=\\B.*)"
            }, {
                "name": "meta.structure.dictionary.value.nt",
                "match": "(?<=:).*$"
            }]
        },
        "tokens": {
            "patterns": [{
                "name": "punctuation.definition.block.sequence.item.nt",
                "match": "â€¢"
            },{
                "name": "punctuation.separator.nt",
                "match": ":"
            }]
        }
    },
    "scopeName": "source.nt"
}